unmapAll
iunmapAll

""""""""""""""""
" META
""""""""""""""""

let configpath = '/home/ross/.cvimrc'
set localconfig

map -: openCommandBar

""""""""""""""""
" TABS
""""""""""""""""

set newtabalwaysontheright

" alt-tab (pun)
map -<A-Tab> lastUsedTab

" merge
map -mp putMergeTabs
map -mt markMergeTab
map -mw markMergeWindow

map -r reloadAllTabs
map -R :source /home/ross/.cvimrc<cr>

""""""""""""""""
" SCROLLING
""""""""""""""""

map `` lastScrollPosition
