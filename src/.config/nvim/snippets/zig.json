{
	"print": {
		"prefix": "p",
		"body": [ "std.debug.print(\"$1\", .{$2});" ]
	},

	"print plain": {
		"prefix": "pp",
		"body": [ "std.debug.print(\"$0\", .{});" ]
	},

	"print text": {
		"prefix": "pt",
		"body": [ "std.debug.print(\"$0\", .{});" ]
	},

	"import": {
		"prefix": "i",
		"body": [ "const $1 = @import(\"./$2\");" ]
	}
}
